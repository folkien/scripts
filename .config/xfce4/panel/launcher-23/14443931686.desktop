[Desktop Entry]
Encoding=UTF-8
Name=XChat IRC
Comment=Chat with other people using Internet Relay Chat
Exec=sh -c "xchat --existing --url %U || exec xchat"
Icon=xchat
Terminal=false
Type=Application
Categories=Network;
StartupNotify=true
MimeType=x-scheme-handler/irc;x-scheme-handler/ircs;
X-XFCE-Source=file:///usr/share/applications/xchat.desktop
